{
	// Quick setup
	// Vim
	"vim.leader": "<space>",
	"vim.enableNeovim": true,
	"vim.neovimPath": "C:\\Neovim\\bin\\nvim.exe",
	"vim.surround": false,
	"vim.easymotion": true,
	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	"vim.hlsearch": true,
	"vim.easymotionMarkerForegroundColorOneChar": "#ff6bcb",
	"vim.searchHighlightColor": "#FDE18",
	"vim.highlightedyank.enable": false,
	"vim.joinspaces": false,
	"vim.mouseSelectionGoesIntoVisualMode": false,

	"vim.insertModeKeyBindingsNonRecursive": [
		{
			"before": ["<C-n>"],
			"commands": ["workbench.action.files.newUntitledFile"]
		},
		{
			"before": ["<C-o>"],
			"commands": ["workbench.action.files.openFile"]
		}
	],

	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": ["<leader>", "d"],
			"after": ["\"", "_", "d"]
		}
	],

	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["<leader>", "d"],
			"after": ["\"", "_", "d"]
		},
		{
			"before": ["D"],
			"after": ["1", "0", "j"]
		},
		{
			"before": ["U"],
			"after": ["1", "0", "k"]
		},
		{
			"before": ["<C-v>"],
			"after": ["p"]
		},
		{
			"before": ["<C-c>"],
			"after": ["yy"]
		},
		{
			"before": ["t", "a"],
			"after": ["f", ">", "a"]
		},
		{
			"before": ["t", "i"],
			"after": ["f", ">", "i"]
		},
		{
			"before": ["f", "f"],
			"after": ["<leader>", "<leader>", "2", "s"]
		},

		// Command mapping
		{
			"before": ["<leader>", "l"],
			"commands": ["workbench.action.navigateRight"]
		},
		{
			"before": ["<leader>", "h"],
			"commands": ["workbench.action.navigateLeft"]
		},
		{
			"before": ["<leader>", "j"],
			"commands": ["workbench.action.navigateDown"]
		},
		{
			"before": ["<leader>", "k"],
			"commands": ["workbench.action.navigateUp"]
		},
		{
			"before": ["<Tab>"],
			"commands": [":bNext"]
		},
		{
			"before": ["<S-Tab>"],
			"commands": [":bprevious"]
		},
		{
			"before": ["<leader>", "w"],
			"commands": ["workbench.action.closeOtherEditors"]
		},
		{
			"before": ["r", "h"],
			"commands": [":nohl"]
		}
	],

	"vim.handleKeys": {
		"<C-a>": false,
		"<C-f>": false,
		"<C-b>": false,
		"<C-w>": false,
		"<C-c>": false,
		"<C-x>": false,
		"<C-d>": false,
		"<C-l>": false
	},

	// editor
	"editor.suggestSelection": "first",
	"editor.fontFamily": "Cascadia Code",
	"editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",

	"editor.fontWeight": 400,
	"editor.fontSize": 18,
	"editor.tabSize": 2,

	"editor.mouseWheelZoom": true,
	"editor.formatOnSave": true,
	"editor.accessibilitySupport": "off",
	"editor.inlineSuggest.enabled": true,
	"editor.wordWrap": "on",
	"editor.renderWhitespace": "none",
	"editor.lineNumbers": "relative",
	"editor.occurrencesHighlight": false,
	"editor.cursorWidth": 2,
	"editor.cursorBlinking": "expand",
	"editor.suggest.preview": true,
	"editor.codeActionsOnSave": {
		"source.organizeImports": true
	},
	"editor.minimap.enabled": false,
	"editor.renderLineHighlight": "all",
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.smoothScrolling": true,

	"editor.bracketPairColorization.enabled": true,
	"editor.tokenColorCustomizations": {
		"[Dracula]": {
			"textMateRules": [
				{
					"scope": ["string.quoted", "string.quoted.single"],
					"settings": {
						"foreground": "#d38a60"
					}
				},
				{
					"scope": "string.template",
					"settings": {
						"foreground": "#f9c9c9",
						"fontStyle": "italic"
					}
				},
				{
					"scope": [
						"entity.other.attribute-name",
						"entity.name.function",
						"support.function",
						"support.function.css",
						"support.function.transform.css",
						"support.type.media-names.css",
						"support.type.property-names.css",
						"entity.other.attribute-name.jsx",
						"entity.other.attribute-name.html",
						"variable.other.class.js"
					],
					"settings": {
						"foreground": "#3FC661",
						"fontStyle": "italic"
					}
				},
				{
					"scope": [
						"variable.other.property.js",
						"variable.other.property.static.js",
						"JSXNested",
						"variable.other.readwrite.js",
						"punctuation.semi-colon",
						"punctuation.colon"
					],
					"settings": {
						"foreground": "#fff"
					}
				},
				{
					"scope": ["punctuation.definition.tag"],
					"settings": {
						"foreground": "#bd93f9"
					}
				},
				{
					"scope": [
						"variable.graphql",
						"string",
						"punctuation.definition.string.begin",
						"punctuation.definition.string.end",
						"meta.attribute-selector.scss",
						"meta.attribute-selector.css"
					],
					"settings": {
						"foreground": "#EAC394"
					}
				},
				{
					"scope": [
						"keyword",
						"storage",
						"constant.length.units.css",
						"source.inside-js.css.styled",
						"keyword.other.unit.percentage.css",
						"keyword.other.unit.px.css",
						"keyword.other.unit.rem.css",
						"constant.length.units.css",
						"constant.percentage.units.css",
						"constant.time.units.css",
						"constant.angle.units.css",
						"constant.numeric.css",
						"punctuation.definition.keyword",
						"entity.name.tag"
					],
					"settings": {
						"foreground": "#e967d7"
					}
				},
				{
					"scope": [
						"variable.parameter",
						"variable.other.constant",
						"support.variable",
						"constant",
						"support.type.property-name.media.css",
						"meta.property-values.css",
						"support.constant"
					],
					"settings": {
						"foreground": "#9a86fd",
						"fontStyle": ""
					}
				},
				{
					"scope": ["support.class.component"],
					"settings": {
						"fontStyle": ""
					}
				},
				{
					"scope": ["variable.other.constant.object.js"],
					"settings": {
						"foreground": "#d2b1ff"
					}
				},
				{
					"scope": ["entity.name.type.parameter"],
					"settings": {
						"foreground": "#ffb86c"
					}
				},
				{
					"scope": ["constant.numeric"],
					"settings": {
						"foreground": "#e9e640",
						"fontStyle": "bold"
					}
				},
				{
					"scope": [
						"support",
						"entity.name.type",
						"meta.interface.ts",
						"entity.other.inherited-class"
					],
					"settings": {
						"foreground": "#33ccc4"
					}
				},
				{
					"scope": [
						"variable.other.readwrite.js",
						"entity.name.type.class",
						"variable.other.property.js",
						"variable.scss",
						"storage.modifier.ts",
						"variable.other.property.static.js"
					],
					"settings": {
						"foreground": "#81b9ee"
					}
				},
				{
					"scope": ["meta.import variable.other.readwrite.alias"],
					"settings": {
						"foreground": "#eb7900"
					}
				},
				{
					"scope": ["string.comment.buffered.block.pug"],
					"settings": {
						"foreground": "#6272A4"
					}
				}
			]
		}
	},

	// workbench
	"workbench.colorCustomizations": {
		"sideBar.background": "#191932",
		"editor.background": "#191932",
		"terminal.background": "#191932",
		"panel.background": "#191932",
		"activityBar.background": "#191932",
		"tab.activeBorderTop": "#8d92ff",
		"tab.inactiveBackground": "#191A22",
		"panelTitle.activeBorder": "#8d92ff",
		"tab.activeBackground": "#191932",
		"breadcrumb.background": "#191932",
		"sideBarSectionHeader.background": "#191932",
		"activityBarBadge.background": "#ff6bcb",
		"activityBar.activeBorder": "#ff6bcb",
		"sideBar.border": "#bd93f9",
		"titleBar.activeBackground": "#1E1F35",
		"terminal.ansiGreen": "#20e3b2",
		"terminal.ansiYellow": "#FDE181",
		"terminal.ansiMagenta": "#ff6bcb",
		"editorCursor.foreground": "#4ece58",
		"editor.selectionBackground": "#135564",
		"editor.selectionHighlightBackground": "#135564",
		"statusBar.background": "#282851",
		"editorGroupHeader.tabsBackground": "#282851",
		"tab.unfocusedHoverBackground": "#333367",
		"breadcrumb.foreground": "#dba264"
	},
	"workbench.activityBar.visible": true,
	"workbench.colorTheme": "Dracula",
	"workbench.iconTheme": "material-icon-theme",
	"window.menuBarVisibility": "toggle",

	// formater
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features",
		"editor.matchBrackets": "never"
	},
	"[css]": {
		"editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter",
		"editor.matchBrackets": "never"
	},
	"[scss]": {
		"editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
	},
	"scssFormatter.singleQuote": true,

	// prettier
	"prettier.singleQuote": true,
	"prettier.jsxSingleQuote": true,
	"autoprefixer.formatOnSave": true,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,

	// Indent
	"indentRainbow.colors": [
		"rgba(40, 54, 24,0.3)",
		"rgba(38, 70, 83,0.25)",
		"rgba(42, 157, 143,0.1)",
		"rgba(233, 196, 106,0.1)",
		"rgba(244, 162, 97,0.1)",
		"rgba(231, 111, 81,0.1)",
		"rgba(252, 163, 17,0.1)"
	],

	// emmet
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"markdown": "html"
	},
	"emmet.triggerExpansionOnTab": true,

	// cSpell
	"cSpell.enabledLanguageIds": [
		"go",
		"graphql",
		"html",
		"java",
		"javascript",
		"javascriptreact",
		"less",
		"php",
		"python",
		"pug",
		"scss",
		"typescript",
		"typescriptreact"
	],
	"cSpell.diagnosticLevel": "Hint",
	"cSpell.userWords": ["highscore", "leaderboard"],

	// highlight matching
	"highlight-matching-tag.styles": {
		"opening": {
			"name": {
				"surround": "#e5f560"
			}
		}
	},

	// git
	"git.enableSmartCommit": true,
	"git.decorations.enabled": true,
	"git.autofetch": true,
	"gitblame.statusBarMessageFormat": "GitHub ${author.name} ( ${time.ago} )",

	// advanced-new-file
	"advancedNewFile.exclude": {
		"node_modules": true,
		"dev": true,
		"dist": true,
		"build": true
	},
	"advancedNewFile.showInformationMessages": true,

	// code snap
	"codesnap.showLineNumbers": false,
	"codesnap.backgroundColor": "#fafafa",

	// file browser
	"explorer.confirmDelete": false,
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"files.exclude": {
		"**/.git": true,
		"**/.next": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/node_modules/": true
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/build": true,
		"**/dist": true,
		"**/.git": true,
		"**/.vercel": true,
		"**/.vscode": true,
		"**/*.min.*": true
	},

	// Terminal
	"terminal.integrated.cwd": "${fileDirname}",
	"terminal.integrated.defaultProfile.windows": "PowerShell",
	"terminal.external.windowsExec": "C:\\Users\\Tuan\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
	"terminal.integrated.fontFamily": "CaskaydiaCove NF",
	"terminal.integrated.fontSize": 16,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.letterSpacing": 0.25,

	// setting sync
	"sync.gist": "97c07480ef1b852bbede0000047b2255",
	"sync.syncExtensions": true,
	"sync.forceUpload": true,

